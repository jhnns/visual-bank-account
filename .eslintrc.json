{
    "extends": [
        "peerigon"
    ],
    "root": true,
    "env": {
        "browser": true
    },
    "rules": {
        "valid-jsdoc": "off",
        "jsdoc/valid-types": "off",
        "jsdoc/require-param": "off",
        "jsdoc/no-undefined-types": "off",
        "import/no-unassigned-import": "off",
        "class-methods-use-this": "off"
    }
}